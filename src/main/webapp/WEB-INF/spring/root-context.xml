<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	
	<!-- Root Context: defines shared resources visible to all other web components -->
	<bean id="personValidator" class="com.me.controller.PersonValidator"></bean>
	<bean id="personDao" class="com.me.dao.PersonDAOImpl"></bean>
	<bean id="clientDao" class="com.me.dao.ClientDAOImpl"></bean>
	<bean id="applicantDao" class="com.me.dao.ApplicantDAOImpl"></bean>
	<bean id="jobDao" class="com.me.dao.JobDAOImpl"></bean>
	<bean id="categoryDao" class="com.me.dao.JobCategoryDAOImpl"></bean>
	<bean id="applicationDao" class="com.me.dao.ApplicationDAOImpl"></bean>
</beans>
